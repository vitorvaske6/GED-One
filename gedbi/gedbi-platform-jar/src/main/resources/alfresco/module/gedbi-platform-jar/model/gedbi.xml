<?xml version="1.0" encoding="UTF-8"?>
<!-- Definition of new Model -->
<model name="gedbi:gedbi" xmlns="http://www.alfresco.org/model/dictionary/1.0">
    <!-- Optional meta-data about the model -->
    <description>Printer One Contratos Model</description>
    <author>Vitor Vasconcelos</author>
    <version>1.0</version>

    <!-- Imports are required to allow references to definitions in other models -->
    <imports>
        <!-- Import Alfresco Dictionary Definitions -->
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
        <!-- Import Alfresco Content Domain Model Definitions -->
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
    </imports>

    <!-- Introduction of new namespaces defined by this model -->
    <namespaces>
        <namespace uri="http://www.gedbi.com/model/content/1.0" prefix="gedbi" />
    </namespaces>
    <constraints>
        <constraint name="gedbi:BItInd" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Ambos</value>
                    <value>IGP-M</value>
                    <value>IPCA</value>
                </list>
            </parameter>
        </constraint>

        <constraint name="gedbi:BItDoc" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Ambos</value>
                    <value>Contrato</value>
                    <value>Aditivo</value>
                </list>
            </parameter>
        </constraint>

        <constraint name="gedbi:BItStatus" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Ambos</value>
                    <value>Ativos</value>
                    <value>Inativos</value>
                </list>
            </parameter>
        </constraint>
    </constraints>
    
    <aspects>
        <aspect name="gedbi:gedbi">
            <title>2. Contratos e Aditivos - Printer One - GEDBI</title>
            <properties>

                <property name="gedbi:BIcnpj">
                    <title>CNPJ do Cliente </title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                </property>

                <property name="gedbi:BInomeFantasia">
                    <title>Nome Fantasia </title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                </property>

                <property name="gedbi:BItDocumento">
                    <title>Tipo do Documento</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <constraints>
                        <constraint ref="gedbi:BItDoc"/>
                    </constraints>
                </property>

                <property name="gedbi:BItIndice">
                    <title>Tipo Indice</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <constraints>
                        <constraint ref="gedbi:BItInd"/>
                    </constraints>
                </property>

                <property name="gedbi:BIstatus">
                    <title>Status do Contrato</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <constraints>
                        <constraint ref="gedbi:BItStatus"/>
                    </constraints>
                </property>

                <property name="gedbi:BIdataInicio">
                    <title>Data de Início</title>
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                </property>

                <property name="gedbi:BIdataFim">
                    <title>Data de Fim</title>
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                </property>

                <property name="gedbi:BIvalorMinimo">
                    <title>Valor Global do Aluguel Mínimo</title>
                    <type>d:double</type>
                    <mandatory>false</mandatory>
                </property>

                <property name="gedbi:BIvalorMaximo">
                    <title>Valor Global do Aluguel Máximo</title>
                    <type>d:double</type>
                    <mandatory>false</mandatory>
                </property>
            </properties>
        </aspect>
    </aspects>
</model>